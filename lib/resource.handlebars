<div class="panel panel-white">
    <div class="panel-heading">
        <h4 class="panel-title">
            <a class="block collapsed" data-toggle="collapse" href="#panel_{{uniqueId}}">
                {{#methods}}
                    <span class="badge badge_{{method}}">{{method}}</span>
                {{/methods}}
                <span class="parent">{{parentUrl}}</span>{{relativeUri}}
            </a>
        </h4>
    </div>
    <div id="panel_{{uniqueId}}" class="panel-collapse collapse">
        <div class="panel-body">
            <div class="list-group">
                {{#methods}}
                    <a href="#" data-toggle="modal" data-target="#{{../uniqueId}}_{{method}}" class="list-group-item">
                        <span class="badge badge_{{method}}">{{method}}</span>
                        {{description}}
                    </a>
                {{/methods}}
            </div>
        </div>
    </div>

    {{#methods}}
        <div class="modal fade" id="{{../uniqueId}}_{{method}}">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <h4 class="modal-title" id="myModalLabel">
                            <span class="badge badge_{{method}}">{{method}}</span>
                            <span class="parent">{{../parentUrl}}</span>{{../relativeUri}}
                        </h4>
                    </div>
                    <div class="modal-body">
                        <div class="alert alert-info">{{md description}}</div>

                        <!-- Nav tabs -->
                        <ul class="nav nav-tabs">
                            <li class="active"><a href="#{{../uniqueId}}_{{method}}_request" data-toggle="tab">Request</a></li>
                            {{#if responses}}
                                <li><a href="#{{../../uniqueId}}_{{method}}_response" data-toggle="tab">Response</a></li>
                            {{/if}}
                        </ul>

                        <!-- Tab panes -->
                        <div class="tab-content">
                            <div class="tab-pane active" id="{{../uniqueId}}_{{method}}_request">
                                {{#if ../allUriParameters}}
                                    <h3>URI Parameters</h3>
                                    <ul>
                                        {{#each ../../allUriParameters}}
                                            <li>
                                                <strong>{{this.displayName}}</strong>
                                                <em>({{this.type}})</em>

                                                {{md description}}
                                            </li>
                                        {{/each}}
                                    </ul>
                                {{/if}}

                                {{#if queryParameters}}
                                    <h3>Query Parameters</h3>
                                    <ul>
                                        {{#each queryParameters}}
                                            <li>
                                                <strong>{{@key}}</strong>
                                                <em>
                                                    {{#if this.required}}
                                                      required
                                                    {{/if}}
                                                    {{#if this.enum}}
                                                      one of ({{this.enum}})
                                                    {{else}}
                                                      ({{this.type}})
                                                    {{/if}}
                                                </em>

                                                {{md description}}

                                                {{#if this.example}}
                                                    <p>
                                                        <small>
                                                            <strong>Example:</strong>
                                                            <code>{{this.example}}</code>
                                                        </small>
                                                    </p>
                                                {{/if}}
                                            </li>
                                        {{/each}}
                                    </ul>
                                {{/if}}

                                {{#if body}}

                                    <strong>Body</strong>
                                <ul>
                                    {{#each body}}
                                        <li>
                                        <em>{{@key}}</em>
                                            <ul>
                                        {{#if this.example}}
                                            <li>
                                            <a data-toggle="collapse" href="#request-example{{hash ../../../../uniqueId ../../../method @key}}">
                                                example
                                            </a>

                                            <pre class="panel-collapse collapse" id="request-example-{{hash ../../../../uniqueId ../../../method @key}}">{{highlight this.example}}</pre>
                                            </li>
                                        {{/if}}

                                        {{#if this.schema}}
                                            <li>
                                            <a data-toggle="collapse" href="#request-schema-{{hash ../../../../uniqueId ../../../method @key}}">
                                                schema
                                            </a>

                                            <pre class="panel-collapse collapse" id="request-schema-{{hash ../../../../uniqueId ../../../method @key}}">{{highlight this.schema}}</pre>
                                        </li>
                                        {{/if}}
                                            </ul>
                                        </li>
                                    {{/each}}
                                    </ul>
                                {{/if}}
                            </div>

                            {{#if responses}}
                                <div class="tab-pane" id="{{../../uniqueId}}_{{method}}_response">


                                    {{#each responses}}

                                        <h2>{{@key}}</h2>
                                        {{md this.description}}


                                        {{#if this.headers}}
                                            <strong>Headers</strong>
                                            <ul>
                                                {{#each this.headers}}
                                                    <li><strong>{{@key}}</strong>: {{this.type}}</li>
                                                {{/each}}
                                            </ul>
                                        {{/if}}


                                        {{#if this.body}}
                                            <strong>Body</strong>
                                            <ul>
                                            {{#each this.body}}
                                                <li>
                                                <em>{{@key}}</em>
                                                    <ul>
                                                    {{#if this.example}}
                                                    <li>
                                                            <a data-toggle="collapse" href="#response-example{{hash ../../../../../../uniqueId ../../../../method @key}}">
                                                                example
                                                            </a>

                                                            <pre class="panel-collapse collapse" id="response-example{{hash ../../../../../../uniqueId ../../../../method @key}}">{{highlight this.example}}</pre>
                                                        </li>
                                                    {{/if}}

                                                    {{#if this.schema}}
                                                    <li>
                                                            <a data-toggle="collapse" href="#response-schema{{hash ../../../../../../uniqueId ../../../../method @key}}">
                                                                schema
                                                            </a>

                                                            <pre class="panel-collapse collapse" id="response-schema{{hash ../../../../../../uniqueId ../../../../method @key}}">{{highlight this.schema}}</pre>
                                                        </li>
                                                    {{/if}}
                                                        </ul>
                                            </li>
                                            {{/each}}
                                            </ul>
                                        {{/if}}
                                    {{/each}}
                                </div>
                            {{/if}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    {{/methods}}
</div>

{{#resources}}
    {{> resource}}
{{/resources}}
